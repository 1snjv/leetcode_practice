#include<bits/stdc++.h>
#include<algorithm>
using namespace std;
int main()
 {
	int t,n,i,j,k;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    int sum=0;
	    vector<int> a(n);
	    for(i=0;i<n;i++)
	    {cin>>a[i];
	        sum+=a[i];
	    }
	    vector<vector<bool>>dp(n+1,vector<bool>(sum+1,false));
	    for(i=0;i<n;i++)
	        dp[i][0]=true;
	    for(i=1;i<=n;i++)
	    {
	        for(j=0;j<=sum;j++)
	        {
	            dp[i][j]=dp[i-1][j];
	            if(a[i-1]<=j)
	            dp[i][j]  = dp[i][j] |dp[i-1][j-a[i-1]];
	        }
	    }
	    for(j=sum/2;j>=0;j--)
	    {
	        if(dp[n][j])
	            break;
	    }
	    cout<<sum - (2*j)<<endl;
	}
	return 0;
}
